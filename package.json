    "builtin-modules": "^5.0.0",
    "clean-css": "^5.3.3",
    "cross-env": "^10.1.0",
    "esbuild": "^0.27.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "madge": "^8.0.0",
    "obsidian": "^1.10.3",
    "ts-jest": "^29.4.5",
